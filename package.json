{
  "name": "ele-list-page",
  "version": "4.1.4",
  "description": "Super list page -- a Vue plug-in based on element UI",
  "main": "dist/build.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build-vue": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build-css": "cross-env NODE_ENV=production NODE_TYPE=css webpack --progress --hide-modules",
    "build": "npm run build-css && npm run build-vue",
    "pub": "npm run build && npm publish && node publish-webhook.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiandaoyi/ele-list-page.git"
  },
  "author": "daoyi.tian",
  "license": "ISC",
  "keywords": [
    "js",
    "vue",
    "element-ui",
    "list-page",
    "table"
  ],
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.0.0-beta.0",
    "element-ui": "^2.15.7",
    "style-loader": "^2.0.0",
    "vue": "^2.5.11"
  },
  "devDependencies": {
    "axios": "^1.6.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "url-loader": "^4.1.1",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
